{
  "name": "firecrawl-scraper-js",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.ts",
  "scripts": {
    "start": "nodemon --exec ts-node src/index.ts",
    "start:production": "tsc && node dist/index.js",
    "format": "prettier --write \"src/**/*.(js|ts)\"",
    "flyio": "node dist/src/index.js",
    "start:dev": "nodemon --exec ts-node index.ts",
    "build": "webpack --mode production",
    "develop": "webpack-dev-server --mode development",
    "test": "npx jest --detectOpenHandles --forceExit --openHandlesTimeout=120000 --watchAll=false --testPathIgnorePatterns='src/__tests__/e2e_noAuth/*'",
    "test:local-no-auth": "npx jest --detectOpenHandles --forceExit --openHandlesTimeout=120000 --watchAll=false --testPathIgnorePatterns='src/__tests__/e2e_withAuth/*'",
    "test:prod": "npx jest --detectOpenHandles --forceExit --openHandlesTimeout=120000 --watchAll=false --testPathIgnorePatterns='src/__tests__/e2e_noAuth/*'",
    "workers": "nodemon --exec ts-node src/services/queue-worker.ts",
    "worker:production": "node dist/src/services/queue-worker.js",
    "mongo-docker": "docker run -d -p 2717:27017 -v ./mongo-data:/data/db --name mongodb mongo:latest",
    "mongo-docker-console": "docker exec -it mongodb mongosh",
    "run-example": "npx ts-node src/example.ts"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@flydotio/dockerfile": "^0.4.10",
    "@tsconfig/recommended": "^1.0.3",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.12",
    "@types/turndown": "^5.0.4",
    "body-parser": "^1.20.1",
    "express": "^4.18.2",
    "jest": "^29.6.3",
    "jest-fetch-mock": "^3.0.3",
    "mammoth": "^1.7.2",
    "nodemon": "^2.0.20",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.4.2",
    "webpack": "^5.92.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.600.0",
    "ajv": "^8.12.0",
    "async": "^3.2.5",
    "async-mutex": "^0.4.0",
    "axios": "^1.3.4",
    "bottleneck": "^2.19.5",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "cron-parser": "^4.9.0",
    "date-fns": "^2.29.3",
    "dotenv": "^16.3.1",
    "express-rate-limit": "^6.7.0",
    "form-data": "^4.0.0",
    "joplin-turndown-plugin-gfm": "^1.0.12",
    "logsnag": "^0.1.6",
    "luxon": "^3.4.3",
    "md5": "^2.3.0",
    "moment": "^2.29.4",
    "pdf-parse": "^1.1.1",
    "pos": "^0.4.2",
    "robots-parser": "^3.0.1",
    "scrapingbee": "^1.7.4",
    "turndown": "^7.1.3",
    "turndown-plugin-gfm": "^1.0.2",
    "typesense": "^1.5.4",
    "unstructured-client": "^0.9.4",
    "uuid": "^9.0.1",
    "xml2js": "^0.6.2"
  },
  "nodemonConfig": {
    "ignore": [
      "*.docx",
      "*.json",
      "temp"
    ]
  },
  "pnpm": {
    "patchedDependencies": {
      "pdf-parse@1.1.1": "patches/pdf-parse@1.1.1.patch"
    }
  }
}
